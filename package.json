{
    "name": "velocity",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "nuxi dev",
        "dev:prepare": "nuxt prepare",
        "build": "nuxt build",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "eslint": "eslint --format=stylish",
        "eslint:d": "eslint --debug --format=stylish",
        "eslint:fix": "eslint --fix --format=stylish",
        "lint": "bun eslint",
        "lint:d": "bun eslint:d",
        "lint:fix": "bun eslint:fix",
        "lint:inspect": "bunx @eslint/config-inspector build && bunx serve .eslint-config-inspector",
        "postinstall": "nuxt prepare",
        "typecheck": "nuxt prepare && vue-tsc -b --noEmit",
        "clean": "rm -rf dist && nuxt cleanup",
        "clean:all": "rm -rf dist && nuxt cleanup && rm -rf node_modules"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@nuxtjs/supabase": "^2.0.1",
        "@pinia/nuxt": "^0.11.2",
        "@supabase/supabase-js": "^2.76.1",
        "nuxt": "^4.1.3",
        "pinia": "^3.0.3",
        "vue": "^3.5.22",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^6.0.0",
        "@eslint/config-inspector": "^1.3.0",
        "@nuxt/eslint": "^1.9.0",
        "@nuxt/kit": "^4.1.3",
        "@types/bun": "latest",
        "@unocss/nuxt": "^66.5.4",
        "eslint": "^9.37.0",
        "eslint-plugin-es-x": "^9.1.1",
        "eslint-plugin-format": "^1.0.2",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-security-node": "^1.1.4",
        "sass": "^1.93.2",
        "unocss": "^66.5.4",
        "vue-tsc": "^3.1.1"
    }
}
